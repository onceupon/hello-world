{
  "family": "sample-fargate",
  "executionRoleArn": "ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "fargate-app",
          "image": "091953878586.dkr.ecr.us-east-1.amazonaws.com/testgithubaction",
          "cpu": 512,
          "portMappings": [
            {
              "hostPort": 8000,
              "protocol": "tcp",
              "containerPort": 8000
            }
          ],
          "memory": 1024,
          "essential": true,
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/sample-fargate",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "ecs"
            }
          }
      }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048"
}
